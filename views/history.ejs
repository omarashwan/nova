<h1 class="booking-title">Your Booking History</h1>

<div class="booking-history">
  <% if (bookings.length === 0) { %>
  <p class="no-bookings">No bookings yet.</p>
  <% } else { %> <% bookings.forEach(booking => { %>
  <div class="booking-card">
    <p><strong>Car:</strong> <%= booking.carId.name %></p>
    <p>
      <strong>From:</strong> <%= new
      Date(booking.startDate).toLocaleDateString() %>
    </p>
    <p>
      <strong>To:</strong> <%= new Date(booking.endDate).toLocaleDateString() %>
    </p>
  </div>
  <% }) %> <% } %>
</div>
